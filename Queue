//Queue Using Linkedlist
public class MyQueueLinkedList{ 
    static class Node {
        int data;
        Node next;

        Node(int data) {
            this.data = data;
            next = null;
        }
    }
    
    Node front;
    Node rear;
    
    void push(int a){
        Node newnode = new Node(a);
        if(front == null){
            front = rear = newnode;
            
        }else{
            rear.next = newnode;
            rear = newnode;
        }
    }
    
    int pop(){
        if(front == null){
            return -1;
        }
        int res = front.data;
        if(front == rear){
            front = rear = null;
        }else{
            front = front.next;
        }
        return res;
    }
    
    public static void main (String arg[]){
        MyQueueLinkedList q = new MyQueueLinkedList();
        q.push(10);
        q.push(5);
        q.push(9);
        q.push(8);
        System.out.print(q.pop());
    }
}
